            gradT_excess_beta1 = 0.5
            gradT_excess_lambda2 = 1.0
            gradT_excess_beta2 = 0.5
            gradT_excess_dlambda = 1
            gradT_excess_dbeta = 1

      ! need to make convection in envelope more efficient for this
      !gradT_excess_limit = 1d-3
      !gradT_excess_factor = 0.01

      varcontrol_target = 1d-4
      mesh_delta_coeff = 0.75

!      delta_lgT_cntr_limit = 0.01d0
!      delta_lgT_cntr_hard_limit = 0.03d0
      mesh_delta_coeff_for_highT = 0.5d0
      logT_max_for_standard_mesh_delta_coeff = 5.0d0
      logT_min_for_highT_mesh_delta_coeff = 5.5d0
      

      !star_H_mass_min_limit = 0.05    ! STOPPING CONDITION
      log_L_lower_limit = -1.0      ! STOPPING CONDITION

      mixing_length_alpha = 2

      use_Ledoux_criterion = .true.
      alpha_semiconvection = 4d-2
      thermohaline_coeff = 2  

      cool_wind_full_on_T = 9d6
      cool_wind_full_off_T = 1d7 
      cool_wind_RGB_scheme = 'Reimers'
      cool_wind_AGB_scheme = 'Blocker'
      RGB_to_AGB_wind_switch = 1d-4
