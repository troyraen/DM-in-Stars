      !use jina
      set_rates_preference = .true.
      new_rates_preference = 2

!CHANGE SOLAR ABUNDANCES

      set_uniform_initial_composition = .true.
      initial_h1 = 7.155D-1
      initial_h2 = 1.431D-5
      initial_he3 = 4.48698D-5
      initial_he4 = 2.703D-1
      !!for initial Z = <<Z>>
      ! CALCULATED AS FOLLOWS:
        ! These two are set below: initial_z = 0.0142, initial_y = 0.2703
        ! Therefore initial_x = 1-initial_y-initial_z = 0.7155
        ! Use ratios from MIST1 (Choi16) and set
        ! initial_h1 = initial_x, initial_h2 = 2D-5*initial_h1
        ! initial_he4 = initial_y, initial_he3 = 1.66D-4*initial_he4

      initial_zfracs = 6 ! AGSS09_zfracs

!!! END MIST

/ ! end of star_job namelist

&controls

!	STOPPING CONDITIONS
      log_L_lower_limit = -1.0
	  stop_at_TP = .TRUE.
